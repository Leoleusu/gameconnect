<h1 class="text-center">投稿一覧</h1>
<div class="container">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>投稿ID</th>
        <th>投稿内容</th>
      </tr>
    </thead>
    <tbody>
      <% @posts.each do |po| %>
        <tr class="moves-contents" onclick='window.location="/admin/posts/<%= po.id %>"' role="link">
          <td><%= po.id %></td>
          <td><%= po.body %></td>
        </tr>
        <% end %>
    </tbody>
  </table>
</div>