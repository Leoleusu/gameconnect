<h1>投稿一覧</h1>
<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th>投稿ID</th>
        <th>投稿内容</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @posts.each do |po| %>
        <tr>
          <td><%= po.id %></td>
          <td><%= po.body %></td>
          <td><%= link_to "詳細",admin_post_path(po.id) %></td>
        </tr>
        <% end %>
    </tbody>
  </table>
</div>